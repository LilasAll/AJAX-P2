
<table class="table table-striped table-dark email-table">
  <thead>
    <tr>
      <th scope="col">#Email</th>
      <th scope="col">Liste des emails</th>
      <th scope="col">Email sélectionné</th>
    </tr>
  </thead>
  <tbody>
    <% Email.all.each do  |email|%>
    <tr>
      <th scope="row"><%= email.id %></th>
      <td><%= email.object %></td>
      <td><%= email.body %> </td>
    </tr>
    <% end %>
  </tbody>
</table>

